cr:hasCategory
  rdf:type owl:ObjectProperty ;
  rdfs:domain cr:Resource ;
  rdfs:label "hasCategory" ;
  rdfs:range ci:Category ;
.
cterm:Aspect
  rdf:type owl:Class ;
  rdfs:label "Aspect" ;
  rdfs:subClassOf cterm:Terminology ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxQualifiedCardinality "5"^^xsd:nonNegativeInteger ;
      owl:onClass cterm:Aspect ;
      owl:onProperty cterm:hasAspect ;
    ] ;
.
cterm:AspectOfEquipment
  rdf:type owl:Class ;
  rdfs:label "AspectOfEquipment" ;
  rdfs:subClassOf cterm:Aspect ;
.
cterm:AspectOfMaterial
  rdf:type owl:Class ;
  rdfs:label "AspectOfMaterial" ;
  rdfs:subClassOf cterm:Aspect ;
.
cterm:AspectOfWorkOfConstructionWork
  rdf:type owl:Class ;
  rdfs:label "AspectOfWorkOfConstructionWork" ;
  rdfs:subClassOf cterm:Aspect ;
  owl:disjointWith cterm:AspectOfWorkOfProductWork ;
  owl:disjointWith cterm:AspectOfWorkOfTemporaryWork ;
.
cterm:AspectOfWorkOfProductWork
  rdf:type owl:Class ;
  rdfs:label "AspectOfWorkOfProductWork" ;
  rdfs:subClassOf cterm:Aspect ;
  owl:disjointWith cterm:AspectOfWorkOfConstructionWork ;
  owl:disjointWith cterm:AspectOfWorkOfTemporaryWork ;
.
cterm:AspectOfWorkOfTemporaryWork
  rdf:type owl:Class ;
  rdfs:label "AspectOfWorkOfTemporaryWork" ;
  rdfs:subClassOf cterm:Aspect ;
  owl:disjointWith cterm:AspectOfWorkOfConstructionWork ;
  owl:disjointWith cterm:AspectOfWorkOfProductWork ;
.
cterm:Category
  rdf:type owl:Class ;
  rdfs:label "Category" ;
  rdfs:subClassOf cterm:Terminology ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass cterm:Category ;
      owl:onProperty cterm:hasCategory ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
.
cterm:CategoryOfActivity
  rdf:type owl:Class ;
  rdfs:label "CategoryOfActivity" ;
  rdfs:subClassOf cterm:Category ;
  owl:disjointWith cterm:CategoryOfCostComponent ;
  owl:disjointWith cterm:CategoryOfWorkOfConstructionWork ;
  owl:disjointWith cterm:CategoryOfWorkOfProductWork ;
  owl:disjointWith cterm:CategoryOfWorkOfTemporaryWork ;
.
cterm:CategoryOfCostComponent
  rdf:type owl:Class ;
  rdfs:label "CategoryOfCostComponent" ;
  rdfs:subClassOf cterm:Category ;
  owl:disjointWith cterm:CategoryOfActivity ;
  owl:disjointWith cterm:CategoryOfWorkOfConstructionWork ;
  owl:disjointWith cterm:CategoryOfWorkOfProductWork ;
  owl:disjointWith cterm:CategoryOfWorkOfTemporaryWork ;
.
cterm:CategoryOfEquipment
  rdf:type owl:Class ;
  rdfs:label "Category of equipment" ;
  rdfs:subClassOf cterm:Category ;
.
cterm:CategoryOfLabour
  rdf:type owl:Class ;
  rdfs:label "Category of labour" ;
  rdfs:subClassOf cterm:Category ;
.
cterm:CategoryOfMaterial
  rdf:type owl:Class ;
  rdfs:label "Category of material" ;
  rdfs:subClassOf cterm:Category ;
.
cterm:CategoryOfWorkOfConstructionWork
  rdf:type owl:Class ;
  rdfs:label "CategoryOfWorkOfConstructionWork" ;
  rdfs:subClassOf cterm:Category ;
  owl:disjointWith cterm:CategoryOfActivity ;
  owl:disjointWith cterm:CategoryOfCostComponent ;
  owl:disjointWith cterm:CategoryOfWorkOfTemporaryWork ;
  owl:equivalentClass cterm:CategoryOfWorkOfProductWork ;
.
cterm:CategoryOfWorkOfProductWork
  rdf:type owl:Class ;
  rdfs:label "CategoryOfWorkOfProductWork" ;
  rdfs:subClassOf cterm:Category ;
  owl:disjointWith cterm:CategoryOfActivity ;
  owl:disjointWith cterm:CategoryOfCostComponent ;
  owl:disjointWith cterm:CategoryOfWorkOfTemporaryWork ;
  owl:equivalentClass cterm:CategoryOfWorkOfConstructionWork ;
.
cterm:CategoryOfWorkOfTemporaryWork
  rdf:type owl:Class ;
  rdfs:label "CategoryOfWorkOfTemporaryWork" ;
  rdfs:subClassOf cterm:Category ;
  owl:disjointWith cterm:CategoryOfActivity ;
  owl:disjointWith cterm:CategoryOfCostComponent ;
  owl:disjointWith cterm:CategoryOfWorkOfConstructionWork ;
  owl:disjointWith cterm:CategoryOfWorkOfProductWork ;
.
cterm:Terminology
  rdf:type owl:Class ;
  rdfs:label "Terminology" ;
  rdfs:subClassOf owl:Thing ;
.
cterm:hasAspect
  rdf:type owl:ObjectProperty ;
  rdfs:domain ci:Work ;
  rdfs:domain cr:EquipmentResource ;
  rdfs:domain cr:MaterialResource ;
  rdfs:label "hasAspect" ;
  rdfs:range cterm:Aspect ;
.
cterm:hasCategory
  rdf:type owl:ObjectProperty ;
  rdfs:domain ci:Activity ;
  rdfs:domain ci:CostComponent ;
  rdfs:domain ci:Work ;
  rdfs:label "hasCategory" ;
  rdfs:range cterm:Category ;
.
